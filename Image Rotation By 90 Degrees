package q13363;
import java.util.*;
public class CTJ13363{
	public static void main(String[] args){
		Scanner scanner=new Scanner(System.in);
		int s=scanner.nextInt();
		int[][] matrix=new int[s][s];
		for(int i=0;i<s;i++){
			for(int k=0;k<s;k++){
				matrix[i][k]=scanner.nextInt();
			}
		}
		for(int i=0;i<s;i++){
			for(int k=s-1;k>=0;k--){
				System.out.print(matrix[k][i]+" ");
			}
			System.out.print("\n");
		}
	}
}
